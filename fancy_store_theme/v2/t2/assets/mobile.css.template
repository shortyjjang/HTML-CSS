.figure-item .info .frm p {display:inline-block;}
.figure-item .info .frm p.option {width: 198px;margin-right: 8px;}
.figure-item .info .frm p.qty {width: 70px;}
.figure-item .info .btn-cart {display:block;width: 100%;margin-top: 10px;}
.figure-item .info .detail dt {font-weight:bold;font-size:14px;color:#282b31;padding:25px 0 3px;}
.figure-item .info .detail a {color:#282b31}
.figure-item .info .detail dd {font-size:13px;line-height:18px;color:#282b31;}
.figure-item .info .detail h2 {font-size:1em}
.figure-item .info .detail div+div {margin-top:20px;}

.merchant-about {padding-bottom:20px;}
.merchant-about .introduce {position:relative;top:10px;margin:0 10px;background:#fff;padding:15px;font-size:14px;line-height:20px;color:#282b31;}
.merchant-about .title {font-size:15px;color:#282b31;padding-bottom:12px;}

.merchant-contact {padding-bottom:20px;}
.merchant-contact .frm {position:relative;top:10px;margin:0 10px;background:#fff;padding:15px;font-size:14px;line-height:20px;color:#282b31;}
.merchant-contact .title {font-size:15px;color:#282b31;padding-bottom:12px;}
.merchant-contact .frm p {padding-bottom:8px;}
.merchant-contact .text {width:100%;}
.merchant-contact .buttons {width:100%;margin-top:7px;}
.merchant-contact address, .merchant-contact .title small {display:none;}

.order {padding-bottom:20px;}
.order .cart {position:relative;top:10px;}
.cart {margin:0 10px;background:#fff;padding:0 15px 15px;font-size:14px;line-height:20px;color:#282b31;}
.cart h2.title {font-size:15px;color:#282b31;padding:15px 0;line-height:20px;border-bottom:1px solid #232323;}
.order-list li {position:relative;font-size:15px;padding:22px 0 20px;border-bottom:0.1em solid #f3f3f3;}
.order-list .product {position:relative;padding:0 20px 0 85px;min-height:75px;padding-bottom:10px;}
.order-list .product img {position:absolute;top: 38px;left: 38px;max-width: 76px;max-height: 76px;background:no-repeat 50% 50%;background-size:contain;transform: translate(-50%, -50%);-webkit-transform: translate(-50%, -50%);-moz-transform: translate(-50%, -50%);}
.order-list .option {width:180px;margin-right:2px;}
.order-list .title {font-weight:normal;}
.order-list .price {font-weight:bold;}
.order-list .qty {width:80px;}
.cart .order-list .remove {position:absolute;top:22px;right:0;width:9px;height:9px;background-position:-70px -40px;text-indent:-1000em;}
.cart .sub-total {text-align:right;font-size:12px;line-height:20px;padding:11px 0 21px;}
.cart .btn-checkout {display:block;width:100%;}

.list .search .sort {float:left;font-size: 13px;}
.list .search h2 {text-transform:uppercase;padding-bottom:0;display: none;}
.list .search li {position:relative;float:left;margin-left: 15px;z-index:1;line-height:22px;}
.list .search a {display:block;white-space:nowrap;padding:8px 0;color: #000;}
.list .search small a:hover {text-decoration:underline;}
.list .search small {position:absolute;background:#fff;padding:6px 14px;display:none;}
.list .search small:before {content:'';position:absolute;left:0;top:-30px;width:100%;height:30px;background:rgba(0,0,0,0)}
.list .search li:hover small {display:block;}
.list .search fieldset {position:relative;clear: both;width: 100%;}
.list .search input {background:none;width: 100%;padding: 9px 15px;border: 1px solid transparent;box-sizing: border-box;}
.list .search .btn-search {position:absolute;width:12px;height:12px;right: 7px;top:50%;margin-top: -7px;background-position:-80px -60px;text-indent:-1000em;display: none;}
.list .search.focus .btn-search {display:block;}
.list .search .btn-search:hover {background-position:-75px -40px}
.list .search.focus input {border-color:#282b31;}
.list .search .mn-sort:after {content:'';display:inline-block;border:3px solid transparent;border-top-color:#6c6c6c;margin-left:5px;}

#widget {position: absolute;padding:0;min-width: 320px;z-index: 4;}
#widget .btn-cart, #widget .btn-cart:hover, #widget .btn-cart:active {position: fixed;right: 50px;top: 15px;width:30px;height:20px;padding:0;box-shadow:none;text-indent:-1000em;z-index: 4;background: none;transition:right 0.22s;-webkit-transition:right 0.22s;-moz-transition:right 0.22s;}
#widget .btn-cart .icon, #widget .btn-cart:hover .icon {position:absolute;left:0;top:0;width:20px;height:20px;background-position:0 -60px;margin:0;}
#widget .btn-cart .count, #widget .btn-cart:hover .count {position:absolute;right:0;top: -8px;margin:0;font-size:11px;padding:0 3px;min-width:11px;text-align:center;line-height:17px;border-radius:9px;background:#fff600;text-indent:0;color:#232323;}
#widget .btn-cart .count:before, #widget .follow-user-link, #widget .btn-fancy .follow, #widget .btn-fancy .following,  #widget .btn-dashboard, #widget .btn-customize {display:none;}
#widget .cart_popup:before {display:none;}
#widget .cart_popup {position:fixed;transform:none;-webkit-transform:none;box-shadow: 0 1px 0 #e5e5e5, 0 -1px 0 #e5e5e5;right:0;top:51px;width:100%;border-radius:0;min-width:320px;height:0;max-height: 90%;max-height: calc(100% - 61px);max-height: -webkit-calc(100% - 61px);max-height: -moz-calc(100% - 61px);overflow: auto;}
#widget .cart_popup.show {height:auto;}

@media screen and (-webkit-min-device-pixel-ratio:2){
	.btn-nav, #widget .btn-cart .icon, #header .search:before, .cart .order-list .remove {background-image:url('{{'m_theme2x.png'|asset_url}}');}
	.collection-list .paging a {box-shadow:inset 0 0 0 0.0.02em #fff;}
	input.text, textarea.text {border-width:0.085em;}
}

/* sales price */
.cart .order-list .price.sales, .figure-list .figcaption .price.sales, .figure-item .info .price.sales {color: #ca2027;}
.cart .order-list .price.sales small.before, .figure-list .figcaption .price.sales small.before, .figure-item .info .price.sales small.before {display:inline;font-size:1em;color: #b0b0b0;text-decoration:line-through;font-weight:normal;}