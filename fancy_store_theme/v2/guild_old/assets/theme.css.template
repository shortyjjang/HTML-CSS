#header .navigation {padding-bottom:27px;text-align:center;}
#header .navigation b {display:block;height:0;overflow:hidden;}
#header .navigation li {display:inline-block;padding:0 22px;}
#header .navigation a {opacity:0.5;}
#header .navigation a em {font-style:normal;font-weight:bold;}
#header .navigation a.current, #header .navigation a:hover {opacity:1;}

/* footer */
#footer {clear:both;text-align:center;padding-top: 78px;padding-bottom: 62px;}
#footer .logo {padding: 0 0 30px;}
#footer .copyright {opacity:0.5;padding-top: 10px;}
#footer .sns li {position:relative;display:inline-block;padding:0 7px 0 11px;}
#footer .sns li:before {content:'|';position:absolute;top:50%;left:-1px;width:1px;height:10px;margin-top:-6px;font-size:10px;line-height:1em;}
#footer .sns li:first-child:before {display:none;}

/* popup */
body.fixed {overflow-y:auto;}
body.fixed #wrap {position:fixed;left:0;width:100%;}
#popup_container {display: none;position: fixed;top: 0;left: 0;z-index: 12;width: 100%;height: 100%;background: rgba(48, 51, 57, 0.95);opacity: 0;transition: opacity 0.2s;-moz-transition: opacity 0.2s;-webkit-transition: opacity 0.2s;-o-transition: opacity 0.2s;overflow: scroll;padding: 0 24px 24px 0;}
#popup_container .popup {display:none;}
.popup {background:#fff;border-radius:3px;margin:50px auto 0;}
.popup .popup_close {position:fixed;top:50px;right:50px;width:17px;height:17px;background-position:-40px 0;}
.popup .title {text-align:center;padding:57px 0 18px;width:400px;margin:0 auto;font-weight:normal;text-transform:uppercase;}
.popup .title:after {content:'';display:block;width:400px;margin:40px auto 0;width:400px;height:2px;background:#b29a76;}

/* about */
#popup_container.merchant-about .popup.merchant-about {display:block;}
.popup.merchant-about {width:621px;}
.popup.merchant-about .title small, .popup.merchant-about .logo {display:none;}
.popup.merchant-about .introduce {padding: 50px 75px 70px;}
.popup.merchant-about p+p {margin-top:22px;}
.popup.merchant-about .slogan {border-top:1px solid #f0f0f0;text-align:center;padding:23px 0;}

/* policy */
#popup_container.merchant-policy .popup.merchant-policy {display:block;}
.merchant-policy {width:621px;}
.merchant-policy .title {padding-bottom: 27px;}
.merchant-policy p {line-height:20px;padding-bottom:20px;color:#5d5d5d;}
.merchant-policy .agreement {padding:0 75px 50px;}
.merchant-policy h4 {font-weight:bold;text-transform:uppercase;padding:22px 0 23px;}

/* contact */
#popup_container.merchant-contact .popup.merchant-contact {display:block;}
.popup.merchant-contact {width:621px;}
.popup.merchant-contact .title small {display:block;font-weight:normal;margin-top:-2px;}
.popup.merchant-contact address {border-top:1px solid #f0f0f0;padding:25px 92px;}
.popup.merchant-contact address b, .popup.merchant-contact address span {display:block;line-height:1.3em;}
.popup.merchant-contact fieldset {padding:34px 90px 40px;}
.popup.merchant-contact p {position:relative;margin-bottom:20px;}
.popup.merchant-contact p.error:before {margin:0;top:18px;}
.popup.merchant-contact .text {padding: 12px 13px;width: 415px;}
.popup.merchant-contact .text.msg {height:98px;}
.popup.merchant-contact .btn-send {margin-top:10px;}
.popup.merchant-contact .complete {line-height:0.8em;padding:322px 0;text-align:center;}
.popup.merchant-contact .complete b {display:block;}

/* newsletter */
#popup_container.newsletter .popup.newsletter {display:block;}
.popup.newsletter {width:621px;}
.popup.newsletter fieldset {padding:34px 90px 40px;}
.popup.newsletter p {position:relative;margin-bottom:20px;}
.popup.newsletter p.error:before {margin:0;top:18px;}
.popup.newsletter .text {padding: 12px 13px;width: 415px;}
.popup.newsletter .text.msg {height:98px;}
.popup.newsletter .btn-send {margin-top:10px;}

/* home */
.home .cover {position:relative;height:380px;overflow:hidden;padding-bottom:70px;}
.home .cover .slide {position:relative;height:500px;overflow:auto;white-space:nowrap;font-size:0;}
.home .cover .slide li{width:100%;margin: 0;padding:0;float: none;display: inline-block;height:380px;vertical-align:top;overflow:hidden;}
.home .cover img {width:100%;vertical-align:top;}
.home .cover .paging {position:absolute;width:100%;left:0;top:380px;text-align:center;}
.home .cover .paging a {display:inline-block;width:6px;height:6px;text-indent:-1000em;border-radius:3px;overflow:hidden;}
.home .copy {display:none;}
.featured h3:after {display:none;}
.minimum-list h3 {float:left;width:50%;}
.minimum-list h3:after {content:'|';margin-left:4px;}
.minimum-list li {position:relative;float:left;width:230px;height:230px;margin:16px 0 0 16px;text-align:center;line-height: 230px;display:table;width: calc(25% - 12px);background: #f9f9f9;}
.minimum-list a {vertical-align:middle;position:relative;z-index:1;display:table-cell;width:auto;padding:0 10px;}
.minimum-list img {width:100%;height:100%;background: no-repeat 50% 50% #f9f9f9;background-size: contain;background-position:50% 50%;position: absolute;top: 0;left: 0;background-blend-mode: darken;}
.home .featured li:first-child {width:477px;height:477px;margin-left:0;line-height: 475px;}
.minimum-list li a:after {content:'';opacity:0;content:'';background:rgba(0,0,0,0.7);position:absolute;top:0;left:0;width:100%;height:100%;z-index: 0;}
.collection-list li a:after {height:230px;}
.minimum-list a:hover:after, .collection-list a:hover:after {opacity:1;}
.minimum-list .price:after {position:absolute;content:'';width:60px;height:1px;top: -11px;left:50%;margin-left:-30px;opacity:0;}
.minimum-list li:hover .price:after, .minimum-list li:hover .title, .minimum-list li:hover .username, .minimum-list li:hover .price {opacity:1;z-index: 1;}
.minimum-list .title {position: relative;font-weight:normal;opacity:0;}
.minimum-list .username {position: relative;opacity:0;}
.minimum-list .price {position: relative;display: block;margin-top: 25px;opacity:0;padding-bottom:7px;}
.minimum-list li:first-child {margin-left:0;}
.minimum-list .after {clear:both;}
.minimum-list .more {float:right;width:50%;text-indent:4px;padding:0;}
.minimum-list .count {position:absolute;top: 115px;left:0;width:100%;text-align:center;margin-top: -12px; height:24px;white-space:nowrap; overflow:hidden;text-overflow:ellipsis; opacity:0;z-index: 1;}
.minimum-list li:hover .count {opacity:1;}
.home .sale {padding: 54px 0 60px;}
.home .featured h3 {width:auto;float:none;text-align:center;}
.home .featured h3:after {display:none;}

/* sub */
.sub #header:after,.sub .search:after {content:'';width:100%;height:1px;display:block;clear:both;}
.search {position:relative;z-index:2;}
.search .sort li {position:relative;float:left;padding:0 20px 0 14px;}
.search .sort a {display:block;padding:0 16px;margin-bottom:7px;}
.search .sort .mn-sort {padding:18px 16px;margin:0;}
.search .sort small {display:none;position:absolute;top:59px;left:14px;border:1px solid rgba(0,0,0,0.06);white-space:nowrap;min-width:137px;background:#fff;padding:10px 0 2px;}
.search .sort li:hover small {display:block;}
.search .sort .mn-sort:after {display:inline-block;vertical-align:middle;margin:2px 0 0 8px;}
.search a.back {padding:18px 27px;float:left;}
.search a.back:before {display:inline-block;vertical-align:middle;margin:-3px 6px 0 0;}
.search fieldset {position:relative;float:right;padding:18px 30px 18px 0;}
.search fieldset input {width:229px;}
.search fieldset.searched:after {background-position: -20px 0px;}
.search fieldset .btn-search {content:'';position:absolute;top:50%;right:31px;width:12px;height:13px;margin-top:-7px;text-indent:-1000em;}
.search fieldset .btn-remove {display:none;width:8px;height:7px;vertical-align:middle;margin-left:3px;background-position:-60px 0;opacity:0.5;text-indent:-1000em;position: absolute;right: 31px;top: 50%;margin-top: -3px;}

.others h3 {text-align:center;}
.others {padding-bottom:33px;}

/* sub > list */
.list .cover, .list .tail-copy {display:none;}
.list .figure-list {clear:both;min-height:600px;}
.list .figure-list h2 {font-weight:normal;padding: 22px 0 8px;}
.list .figure-list h2 small {display:block;}
.list .stream li {float:left;text-align:center;padding: 25px 0 15px 15px;width: 313px;width: calc((100%/3) - 10px);}
.list .stream li:nth-child(3n+1) {padding-left:0;clear:both;}
.list .stream .figcaption {display:block;padding: 12px 0 18px;}
.list .stream .title {display:block;padding: 0 0 9px;font-weight:normal;position: relative;}
.list .stream .title:after {content:'';position:absolute;bottom:0;left:50%;margin-left: -28px;width:56px;height:1px;}
.list .stream .price {display:block;padding: 10px 0 0;}
.list .stream .status {color:#E77471;}
.list .stream .figure {display:block;background: #f9f9f9;}
.list .stream img {width: 100%;height:477px;background: no-repeat 50% 50% #f9f9f9;background-size: contain;vertical-align:top;background-blend-mode: darken;}
.list .collection-list {padding: 52px 0 43px;}
.collection-list h2 {font-weight:normal;width:970px;margin:0 auto;}
.collection .cover img {display:none;}
.collection-list li {height: auto;margin-bottom:30px;}
.collection-list li:nth-child(4n+1) {margin-left:0;}
.collection-list a:before {display:block;}
.collection-list a:before {display:none;}
.collection-list a {position: relative;padding: 0;display: block;}
.collection-list img {display:block;height:230px;position: static;}
.collection-list a:before {height:230px;}
.collection-list .title {position:relative;display:block;opacity: 1;padding:14px 0 10px;z-index: 1;}
.collection-list .title:after {content:'';position:absolute;bottom:0;left:50%;margin-left:-28px;width:56px;height:1px;}
.collection .collection-list {padding-bottom:56px;}

/* sub > product */
.things .figure-item .photo {position:relative;height:500px;width:500px;padding: 23px 0 85px;}
.things .figure-item .zoom {line-height:498px;}
.things .figure-item .zoom img {max-width:500px;max-height:500px;vertical-align:middle;}
.things .figure-item .figure-list {position:absolute;top: 534px;left: 0;}
.things .figure-item .figure-list li {float:left;margin-left:10px;}
.things .figure-item .figure-list li:first-child {margin-left:0;}
.things .figure-item .figure-list a {position:relative;display:block;opacity:0.5;}
.things .figure-item .figure-list img {width: 75px;height: 75px;vertical-align:top;background:no-repeat 50% 50%;background-size:cover;}
.things .figure-item .figure-list a.current {opacity:1;}
.things .figure-item .info {float:right;width:440px;}
.things .figure-item .title {line-height: 1.15em;font-weight:normal;margin: 30px 0 19px;margin-left: -6px;}
.things .figure-item .price {margin-bottom: 26px;}
.things .figure-item .price b {font-weight:normal;}
.things .figure-item .frm p {float:left;padding-bottom:20px;}
.things .figure-item .frm .label {display:block;padding: 4px 0 8px;}
.things .figure-item .frm .btn-cart {clear:both;}
.things .figure-item .frm .option {padding-right:20px;}
.things .figure-item .frm .option select {width: 298px;}
.things .figure-item .frm .qty .text {width:92px;padding: 7px 13px;}
.things .figure-item .frm .qty select {width:118px;}
.things .figure-item .frm .text, .things .figure-item .frm .selectBox {background:#fff;border:1px solid #efefef;}
.things .figure-item .detail {position:relative;padding-top:38px;}
.things .figure-item .detail-menu:after {content:'';clear:both;display:block;}
.things .figure-item .detail-menu li {float:left;line-height:1.4em;padding:0 15px;border-left:2px solid #f0f0f0;}
.things .figure-item .detail-menu li a {position:relative;display:block;}
.things .figure-item .detail-menu li:first-of-type {padding-left:0;border-left:0;}
.things .figure-item .detail-menu li a.current:after {position:absolute;bottom:1px;left:0;width:100%;height:1px;content:'';}
.things .figure-item .detail dl {clear:both;}
.things .figure-item .detail dd {clear:both;display:none;line-height:1.65em;padding-top:18px;}
.things .figure-item .detail dd > div {padding-top:18px;}
.things .figure-item .detail h2 {font-size:1em;}
.things .figure-item .detail dd > div:first-child {padding-top:0;}
.things .figure-item .detail dt {display:none;}
.things .figure-item .detail .current dd {display:block;}
.things .figure-item .detail dd a {text-decoration:underline;}
.similar h3, .others h3 {text-align:center;width: auto;float: none;}
.similar h3:after, .others h3:after {display:none;}
.similar {padding: 50px 0;clear: both;}

#common-widget .cart_popup {right: 10px;background: #FFF;top: 50px;position: absolute;border-radius: 3px;-webkit-box-shadow: 0 3px 11px rgba(0, 0, 0, 0.3),0 0 1px rgba(0, 0, 0, 0.4);-moz-box-shadow: 0 3px 11px rgba(0, 0, 0, 0.3),0 0 1px rgba(0, 0, 0, 0.4);-ms-box-shadow: 0 3px 11px rgba(0, 0, 0, 0.3),0 0 1px rgba(0, 0, 0, 0.4);-o-box-shadow: 0 3px 11px rgba(0, 0, 0, 0.3),0 0 1px rgba(0, 0, 0, 0.4);box-shadow: 0 3px 11px rgba(0, 0, 0, 0.3),0 0 1px rgba(0, 0, 0, 0.4);z-index: 11;opacity: 0;-webkit-transform-origin: 100% 0;transform-origin: 100% 0;transform: scale(0);-webkit-transform: scale(0);transform: scale(0);-webkit-transition: all 0.3s cubic-bezier(0.7, 1.2, 0.5, 1);transition: all 0.3s cubic-bezier(0.7, 1.2, 0.5, 1);}
