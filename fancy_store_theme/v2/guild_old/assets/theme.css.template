* {margin:0;padding:0;border:0;vertical-align:baseline;list-style:none;quotes:none;font-style:normal;outline:none;line-height:1.45em;}
body,textarea,input,button,select {font-family:"Lora", "Georgia","Times New Roman",Times,serif;}
a {text-decoration:none;}
button {cursor:pointer;}
.left {float:left;}
.right {float:right;}
.clear {clear:both;}
.after:after {content:'';width:0;height:0;display:block;clear:both;overflow:hidden;}
.hidden, #accessibility-nav {position:absolute;top:-1000em;left:-1000em;overflow:hidden;width:0;height:0;}
.inner {width:970px;margin:0 auto;}
input.text,textarea.text, .selectBox select {-webkit-appearance: none; -moz-appearance: none; appearance: none;padding: 6px 13px;height:24px;border-radius:3px;}
