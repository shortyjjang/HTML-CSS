p.error {position:relative;}
p.error:before {content:'';position:absolute;top:50%;left:-24px;width:11px;height:11px;background-position:-20px 0;margin-top:-5px;}
p.error .error-msg {position:absolute;top:12px;left:13px;width:415px;}
.buttons {color:#fff;text-align:center;border-radius:3px;line-height:2.9em;width:100%;display:block;}
.buttons[disabled], .buttons:disabled {opacity:0.5;cursor:default;}

#wrap {min-width:970px;}
#loading-indicator {display:none;clear:both;text-align:center;}
