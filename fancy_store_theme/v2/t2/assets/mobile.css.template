@-webkit-keyframes show_widget {from {opacity:0;top:-37px;}to {opacity:1;top:0;}}
@keyframes show_widget{from {opacity:0;top:-37px;}to {opacity:1;top:0;}}


#header {width:100%;float:left;z-index: 3;background: #ffffff;position: fixed;top: 0;left: 0;transition: left 0.22s;  -webkit-transition: left 0.22s;  -moz-transition: left 0.22s;}
#header .logo {float:left;line-height: 30px;padding: 8px 10px;}
#header .logo img {max-height:35px;vertical-align: middle;margin-top:-3px;}
#header .btn-nav {float:right;width:20px;height: 16px;background-position:-80px -40px;margin: 17px 10px 17px 0;}
#header .navigation {visibility: hidden;position: fixed;width:200px;top:0;right:-260px;background:#212121;padding:0 30px;overflow:auto;height:100%;text-transform:uppercase;-webkit-overflow-scrolling:touch;transition: right 0.22s;  -webkit-transition: right 0.22s;  -moz-transition: right 0.22s;}
.show #wrap, .show #header {left:-260px;overflow:visible;}
.show #widget .btn-cart, .show #widget .btn-cart:hover, .show #widget .btn-cart:active {right:310px;}
.show #header .navigation {visibility:visible;right: 0;}
#header .navigation a {display:block;font-size:15px;line-height:36px;color:#f0f0f0;}
#header .navigation small {display:block;opacity:0.4;padding:6px 0;}
#header .navigation small a {font-size:11px;line-height:33px;}
#header .navigation ul {padding:20px 0;border-top:1px solid #2c2c2c;}
#header .navigation ul:first-child {border-top:0;}
#header .navigation .copyright {color:#f0f0f0;font-size:11px;line-height:20px;padding:20px 0;}

#container {clear:both;}
#wrap {overflow:hidden;position:absolute;top:0;left:0;width:100%;min-height:100%;min-width:320px;transition:left 0.22s;-webkit-transition:left 0.22s;-moz-transition:left 0.22s;padding-top: 50px;box-sizing: border-box;}
#content {position:relative;z-index:0;}
#loading-indicator {display:none;clear:both;text-align:center;padding: 30px;}
#loading-indicator span {position:relative;display:inline-block;width:6px;height:6px;text-indent:-1000em;border-radius:5px;line-height:0;margin:0 3px;opacity:0.6;vertical-align:top;;-webkit-animation: loading_dot2 1s infinite;animation: loading_dot2 1s infinite;}
#loading-indicator:before {content:'';display:inline-block;width:6px;height:6px;text-indent:-1000em;border-radius:5px;line-height:0;margin:0 3px;opacity:0.6;vertical-align:top;-webkit-animation: loading_dot 1s infinite;animation: loading_dot 1s infinite;}
#loading-indicator:after {content:'';display:inline-block;width:6px;height:6px;text-indent:-1000em;border-radius:5px;line-height:0;margin:0 3px;opacity:0.6;vertical-align:top;-webkit-animation: loading_dot3 1s infinite;animation: loading_dot3 1s infinite;}
@-webkit-keyframes loading_dot {10% {opacity:0.6;}20% {opacity:1;}30%{opacity:0.6;}100%{opacity:0.6;}}
@keyframes loading_dot {10% {opacity:0.6;}20% {opacity:1;}30%{opacity:0.6;}100%{opacity:0.6;}}
@-webkit-keyframes loading_dot2 {40% {opacity:0.6;}50% {opacity:1;}60%{opacity:0.6;}100%{opacity:0.6;}}
@keyframes loading_dot2 {40% {opacity:0.6;}50% {opacity:1;}60%{opacity:0.6;}100%{opacity:0.6;}}
@-webkit-keyframes loading_dot3 {80% {opacity:0.6;}90% {opacity:1;}100%{opacity:0.6;}}
@keyframes loading_dot3 {80% {opacity:0.6;}90% {opacity:1;}100%{opacity:0.6;}}

#footer {display:none;}

p.error {position:relative;}
p.error .error-msg {position:absolute;background:#fff;color:#c13939;top:1px;left:1px;line-height:18px;padding:8px 7px;font-size:12px;}
