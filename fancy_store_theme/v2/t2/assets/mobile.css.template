@charset "UTF-8";

* {margin:0;padding:0;list-style:none;}
fieldset,img,button,textarea,input {border:0;outline:none;}
button,a {cursor:pointer;}
body,textarea,input,button,select {font-family:"Open Sans", "Helvetica Neue",Helvetica,Arial,sans-serif;}
a {text-decoration:none;}
body,html {overflow-x:hidden;background:#f0f0f0;}

.btn-nav, #widget .btn-cart .icon, #header .search:before, .cart .order-list .remove {background:url('{{'m_theme.png'|asset_url}}') no-repeat;background-size:100px 100px;}

#accessibility-nav,.hidden {position:absolute;width:0;height:0;overflow:hidden;}
.after:after {content:'';clear:both;display:block;height:0;overflow:hidden;}
input.text, textarea.text {border:1px solid #c6c6c6;background:#e7e7e7;-webkit-appearance: none;width: 100%;line-height: 22px;padding: 7px 10px;border-radius:0;}
select {background:none;-webkit-appearance: none;border: 0;width: 100%;height: 32px;padding: 7px 10px;border-radius:0;}
.selectBox {position:relative;display:inline-block;width:100%;border: 1px solid #232323;background:none;}
.selectBox:after {content:'';position:absolute;right:10px;top:50%;border: 5px solid transparent;border-top-color: #373737;margin-top: -2px;}

#widget .after:after {content:'';width:0;height:0;display:block;clear:both;overflow:hidden;}
#widget, #widget * {padding:0;margin:0;border:0;list-style:none;outline:none;font-family:"Helvetica Neue",Helvetica,Arial,sans-serif;}
#widget a {text-decoration:none;}
#widget button, #widget input {cursor:pointer;}
