    <td colspan="6">

        <!-- Payment -->
        <dl class="table">
            <dt>Total Payment</dt>
            <dd><table class="tb-type2">
                <thead>
                    <tr>
                        <th>Status</th>
                        <th>Total Amount</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><%= model.payment_status_display %></td>
                        <td><%= displayFee(model.total_fee) %></td>
