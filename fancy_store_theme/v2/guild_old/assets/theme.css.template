#common-widget .btn-pw:before{width:16px;height:9px;background:#9099a6;border-radius:5px;box-shadow:inset 0 0 0 1px rgba(0,0,0,0.13);margin-top:-5px;}
#common-widget .btn-pw:after {width:7px;height:7px;background:#fff;border-radius:100%;margin:-4px 0 0 1px;box-shadow:0 0 0 1px rgba(0,0,0,0.1);}
#common-widget .btn-pw.on:before {background:#2184dc;}
#common-widget .btn-pw.on:after {margin-left:8px;}

.blog-post {position:relative;max-width:none;padding-bottom:100px;}
.blog-post .cover {position:relative;}
.blog-post .cover:before {content:'';position:absolute;top:0;left:0;width:100%;height:100%;background:#000;opacity:0.3;}
.blog-post .cover img {width:100%;display:block;margin:0 auto;}
.blog-post .cover+.title {position:absolute;top:0;left:0;color:#fff;margin-top:0;}
.blog-post .title {width:100%;display:table;margin-top:50px;}
.blog-post .title .inner {display:table-cell;vertical-align:middle;text-align:center;}
.blog-post .title h2 {font-size:46px;line-height: 53px;padding: 5px 0;}
.blog-post .title p {font-size:24px;line-height: 28px;padding: 3px 0;}
.blog-post .post {max-width:650px;margin:0 auto;background:#fff;color:#141516;font-size:18px;line-height:28px;padding:50px;}
.blog-post .post h1, .blog-post .post h2, .blog-post .post h3 {font-size:26px;line-height:36px;padding:20px 0 70px;text-align:center;font-weight:500;}
.blog-post .post p {padding-bottom:50px;}
.blog-post .date {text-align:center;font-size:11px;font-weight:600;color:#9b9999;line-height:16px;padding:50px 0;}

.blogSection {max-width:970px;margin:0 auto;padding:28px 0;}
.blogSection .tit {font-size:40px;line-height:60px;padding:31px 0 59px;text-align:center;margin:0;border:0;}
.blogList:after {content:'';clear:both;display:block;}
.blogList {margin:0 -10px;width:calc(100% + 20px);width:-webkit-calc(100% + 20px);width:-moz-calc(100% + 20px);}
.blogList .blogElement {float:left;width:48%;width:calc(50% - 20px);width:-webkit-calc(50% - 20px);width:-moz-calc(50% - 20px);margin:0 10px 40px;}
.blogElement {background: #fff;transform: translate3d(0, 0, 0);-webkit-transform: translate3d(0, 0, 0);-webkit-transition: 0.3s;transition: 0.3s;}
.blogElement:hover { box-shadow: 0 22px 43px rgba(0,0,0,0.15); -webkit-box-shadow: 0 22px 43px rgba(0,0,0,0.15); -webkit-transform: translateY(-4px); transform: translateY(-4px); -moz-transform: none; }
.blogElement a {display:block;padding-bottom:25px;}
.blogElement img {display:block;height:244px;background:no-repeat 50% 50%;background-size:cover;width: 100%;box-shadow:inset 2px 2px 0 #fff,inset -2px 0 0 #fff;}
.blogElement .title {display:block;font-size:24px;color:#141516;font-weight:500;margin:0 30px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;padding-top: 25px;}
.blogElement .date {display:block;font-size:12px;font-weight:500;color:#141516;margin:0 30px;padding-top: 3px;opacity: 0.4;}
.blogElement .description {display:block;font-size:15px;line-height:24px;max-height:48px;color:#141516;margin: 0 30px;padding-top: 16px;overflow:hidden;}
.blogElement .more {display:block;font-size:15px;line-height:24px;padding: 10px 0 0;color:#141516;font-weight:500;margin: 0 30px;}
.blogList .blogElement.sm {width:30%;width:calc((100%/3) - 20px);width:-webkit-calc((100%/3) - 20px);width:-moz-calc((100%/3) - 20px);}
.blogElement.banner {display:table;height:330px;padding:0;}
.blogElement.banner img {box-shadow: none;position: absolute;top: 2px;left: 2px;width: calc(100% - 4px);width: -webkit-calc(100% - 4px);width: -moz-calc(100% - 4px);height: 326px;opacity: 0.7;}
.blogElement.banner a {padding: 2px;display:table-cell;vertical-align:middle;text-align:center;box-shadow: inset 0 0 0 2px #fff;background: #000;}
.blogElement.banner .title {padding:0;position: relative;left: 0;bottom: 0;margin: 0;font-size: 24px;font-weight: bold;color:#fff;}
.blogElement.banner .title:before {content:'BLOG';display:block;font-size:13px;font-weight:400;opacity: 0.7;margin-bottom: 7px;}
.blogElement.banner .date, .blogElement.banner .more, .blogElement.banner .description {display:none;}

.related_items {max-width:970px;margin:0 auto;}
.related_items:after {content:'';clear:both;display:block;}
.related_items li {float:left;width:313px;padding:30px 0 27px 15px;text-align:center;width: calc(25% - 12px);}
.related_items li:nth-child(4n+1) {clear:both;padding-left:0;}
.related_items .figcaption {display:block;padding: 12px 0 18px;}
.related_items .title {display:block;padding: 0 0 9px;font-weight:normal;position: relative;height:auto !important;color: #464646;}
.related_items .title:after {content:'';position:absolute;bottom:0;left:50%;margin-left: -28px;width:56px;height:1px;background:#464646;}
.related_items .price {display:block;padding: 10px 0 0;}
.related_items .status {color:#E77471;}
.related_items img {width: 100%;background:no-repeat 50% 50%;background-size:cover;vertical-align:top;}
.related_items .price.sales small.before {font-size:1em;color: #b0b0b0;text-decoration:line-through;font-weight:normal;}
.related_items .price.sales {color: #ca2027;}